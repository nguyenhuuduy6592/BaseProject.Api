{
  "ConnectionStrings": {
    "SQLDB": "[SQL_CONNECTION_STRING]"
  },
  "AllowedHosts": "*",
  "TokenOptions": {
    "Secret": "[TOKEN_SECRET]",
    "RefreshSecret": "[REFRESH_TOKEN_SECRET]",
    "Issuer": "[TOKEN_ISSUER]",
    "Audience": "[TOKEN_AUDIENCE]"
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "HTTP-Api-Basic",
        "Uri": "[SERVICE_URL]/self-health"
      }
    ],
    "EvaluationTimeOnSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  }
}
